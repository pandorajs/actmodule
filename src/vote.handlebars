<div class="lottery-pop" id="vote_pop" style="z-index:9999; top:0; left:0; position:fixed; display:none;">
    <span class="lottery-pop-close"></span>
    <h4 class="lottery-pop-tit">完成投票后参与抽奖</h4>
    <div class="lottery-pcon">
        {{#each votelist}}
        <div class="lottery-vote" data-voteid="{{voteid}}" data-votetype="{{voteflag}}">
            <h4 class="lottery-tit">{{title}}</h4>
            <ul class="lottery-votelist">
                {{#each itemlist}}
                <li class="lottery-votelist-item" data-itemid="{{itemid}}"><div>{{itemtitle}}</div></li>
                {{/each}}
            </ul>
            <div class="lottery-btn-box"><button type="button" class="lottery-btn-submit" id="submit_vote">提交</button></div>
        </div>
        {{/each}}
    </div>
</div>
<div class="lottery-mask" style="position:fixed; top:0; left:0; width:100%; height:100%; background:rgba(0,0,0,.5); z-index:999; display:none;"></div>